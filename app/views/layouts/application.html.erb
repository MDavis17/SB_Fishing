<!DOCTYPE html>
<html lang="en_US">
	<head>
	  <title><%=title%></title>
	  <%= stylesheet_link_tag    'application', media: 'all' %>
	  <%= javascript_include_tag 'application' %>
	  <%= csrf_meta_tags %>
	</head>

	<body>
		<div id="container">
			<%= render 'layouts/header'%>
			<%= render 'layouts/navigation'%>

			<%= yield %>

			<%= render 'layouts/footer'%>
		</div>

	</body>
</html>
